{
  "expo": {
    "name": "test-location-app",
    "slug": "test-location-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.test.testlocationapp",
      "buildNumber": "6",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.test.testlocationapp",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "ACCESS_BACKGROUND_LOCATION", "CAMERA", "FOREGROUND_SERVICE"],
      "versionCode": 6,
      "config": {
        "googleMaps": { "apiKey": "<key here>" }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
